{
  "appId": "com.shapefit.app",
  "appName": "ShapeFit",
  "webDir": "app",
  "server": {
    "androidScheme": "https",
    "hostname": "app.shapefit.com",
    "cleartext": false
  },
  "ios": {
    "contentInset": "never",
    "backgroundColor": "#101010",
    "scheme": "shapefit"
  },
  "android": {
    "backgroundColor": "#101010",
    "allowMixedContent": false,
    "captureInput": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#101010",
      "showSpinner": true,
      "spinnerColor": "#FF6600"
    },
    "Network": {
      "networkStatus": true
    },
    "App": {
      "launchUrl": "index.html"
    }
  }
}










